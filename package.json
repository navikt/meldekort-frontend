{
  "name": "meldekort-frontend",
  "private": true,
  "sideEffects": false,
  "type": "commonjs",
  "scripts": {
    "build": "remix build && npm run build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev -c \"ts-node ./server/server.ts\" --manual",
    "start": "cross-env NODE_ENV=production ts-node ./server/server.js",
    "typecheck": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --fix --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.4.1",
    "@navikt/ds-css": "^5.18.3",
    "@navikt/ds-react": "^5.18.3",
    "@navikt/nav-dekoratoren-moduler": "^2.1.5",
    "@navikt/oasis": "^3.0.0",
    "@remix-run/css-bundle": "^2.7.2",
    "@remix-run/express": "^2.7.2",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.5.0",
    "isbot": "^5.1.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "remix-i18next": "^5.5.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.6.0",
    "@remix-run/eslint-config": "^2.7.2",
    "@remix-run/testing": "^2.7.2",
    "@testing-library/react": "^14.2.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.58",
    "@types/react-dom": "^18.2.19",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.3.0",
    "chokidar": "^3.6.0",
    "eslint": "^8.56.0",
    "msw": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

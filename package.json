{
  "name": "meldekort-frontend",
  "private": true,
  "sideEffects": false,
  "type": "commonjs",
  "scripts": {
    "build": "remix build && npm run build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev -c \"ts-node ./server/server.ts\" --manual",
    "start": "cross-env NODE_ENV=production ts-node ./server/server.js",
    "typecheck": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage"
  },
  "dependencies": {
    "@navikt/dp-auth": "^0.3.17",
    "@navikt/ds-css": "^5.17.1",
    "@navikt/ds-react": "^5.17.1",
    "@navikt/nav-dekoratoren-moduler": "^2.1.4",
    "@remix-run/css-bundle": "^2.6.0",
    "@remix-run/express": "^2.6.0",
    "@remix-run/node": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^7.0.0",
    "i18next": "^23.8.2",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.4.3",
    "isbot": "^4.4.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "remix-i18next": "^5.5.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.6.0",
    "@remix-run/eslint-config": "^2.6.0",
    "@testing-library/react": "^14.2.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.2.53",
    "@types/react-dom": "^18.2.18",
    "@types/source-map-support": "^0.5.6",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.2.2",
    "chokidar": "^3.5.3",
    "eslint": "^8.56.0",
    "msw": "^2.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

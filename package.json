{
  "name": "meldekort-frontend",
  "private": true,
  "sideEffects": false,
  "type": "commonjs",
  "scripts": {
    "build": "remix build && npm run build:server",
    "build:server": "tsc --project server/tsconfig.json",
    "dev": "remix dev -c \"ts-node ./server/server.ts\" --manual",
    "start": "cross-env NODE_ENV=production ts-node ./server/server.js",
    "typecheck": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts,.tsx",
    "lint-fix": "eslint . --fix --ext .ts,.tsx",
    "validate": "npm run lint && tsc",
    "test": "vitest",
    "test-coverage": "vitest --coverage"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.9.3",
    "@navikt/aksel-icons": "^6.13.0",
    "@navikt/ds-css": "^6.13.0",
    "@navikt/ds-react": "^6.13.0",
    "@navikt/nav-dekoratoren-moduler": "^2.1.6",
    "@navikt/oasis": "^3.3.0",
    "@remix-run/css-bundle": "^2.10.3",
    "@remix-run/express": "^2.10.3",
    "@remix-run/node": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "express": "^4.19.2",
    "express-prom-bundle": "^7.0.0",
    "html-react-parser": "^5.1.12",
    "i18next": "^23.12.2",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-backend": "^2.5.2",
    "isbot": "^5.1.13",
    "jsdom": "^24.1.1",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "remix-i18next": "^6.1.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.10.3",
    "@remix-run/eslint-config": "^2.10.3",
    "@remix-run/testing": "^2.10.3",
    "@testing-library/react": "^16.0.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-istanbul": "^2.0.3",
    "chokidar": "^3.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "msw": "^2.3.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "vitest": "^2.0.3"
  },
  "engines": {
    "node": ">=21.0.0"
  }
}
